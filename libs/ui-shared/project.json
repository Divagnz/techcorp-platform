{
  "name": "ui-shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui-shared/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:ng-packagr",
      "outputs": ["{workspaceRoot}/dist/libs/ui-shared"],
      "options": {
        "project": "libs/ui-shared/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui-shared/tsconfig.lib.json"
        },
        "development": {
          "tsConfig": "libs/ui-shared/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ui-shared/jest.config.ts",
        "tsConfig": "libs/ui-shared/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "version": {
      "executor": "nx-project-release:version",
      "options": {}
    },
    "changelog": {
      "executor": "nx-project-release:changelog"
    },
    "publish": {
      "executor": "nx-project-release:publish",
      "dependsOn": ["build"]
    }
  }
}
