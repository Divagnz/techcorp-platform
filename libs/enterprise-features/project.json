{
  "name": "enterprise-features",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/enterprise-features/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:ng-packagr",
      "outputs": ["{workspaceRoot}/dist/libs/enterprise-features"],
      "options": {
        "project": "libs/enterprise-features/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/enterprise-features/tsconfig.lib.json"
        },
        "development": {
          "tsConfig": "libs/enterprise-features/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production",
      "dependsOn": ["^build"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/enterprise-features/jest.config.ts",
        "tsConfig": "libs/enterprise-features/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "version": {
      "executor": "nx-project-release:version",
      "options": {
        "releaseGroup": "libs"
      }
    },
    "changelog": {
      "executor": "nx-project-release:changelog"
    },
    "publish": {
      "executor": "nx-project-release:publish",
      "dependsOn": ["build"]
    }
  }
}
